{
  "name": "ld-scheduler",
  "version": "1.5.0",
  "description": "Schedule launch darkly flags on or off",
  "main": "lib/index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "test": "jest --watchAll",
    "lint": "eslint --cache --format 'node_modules/eslint-friendly-formatter' ./src",
    "build": "rimraf lib/* && babel src -d lib --ignore *.test.js",
    "ui": "babel-node ./src/ui/server",
    "build-ui": "webpack --mode development"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yusinto/ld-scheduler.git"
  },
  "keywords": [
    "schedule",
    "launch",
    "darkly",
    "feature",
    "flags",
    "deployment",
    "automation"
  ],
  "author": "Yus Ng",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yusinto/ld-scheduler/issues"
  },
  "homepage": "https://github.com/yusinto/ld-scheduler#readme",
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.39",
    "@babel/core": "^7.0.0-beta.39",
    "@babel/node": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@babel/preset-react": "^7.0.0-beta.39",
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-loader": "^8.0.0-beta.0",
    "webpack": "4.0.0-beta.0",
    "webpack-cli": "2.0.4",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  }
}
